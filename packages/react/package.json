{
  "name": "@cosmos-kit/react",
  "version": "1.3.32-beta.3",
  "description": "cosmos-kit wallet connector",
  "author": "developers@cosmology.tech",
  "contributors": [
    {
      "name": "Jun Liu"
    },
    {
      "name": "Dan Lynch"
    },
    {
      "name": "Noah Saso"
    },
    {
      "name": "Eliot Baker"
    },
    {
      "name": "Delivan Jeonghyeok Yoo"
    }
  ],
  "homepage": "https://github.com/cosmology-tech/cosmos-kit#readme",
  "license": "SEE LICENSE IN LICENSE",
  "source": "src/index.ts",
  "main": "dist/cosmos-kit-react.cjs.js",
  "module": "dist/cosmos-kit-react.esm.js",
  "types": "dist/cosmos-kit-react.d.ts",
  "css": "dist/styles.css",
  "targets": {
    "main": {
      "source": "src/index.ts"
    },
    "module": {
      "source": "src/index.ts"
    },
    "types": {
      "source": "src/index.ts"
    },
    "css": {
      "source": "src/styles.css"
    }
  },
  "exports": {
    ".": {
      "types": {
        "import": "./dist/cosmos-kit-react.d.ts",
        "default": "./dist/cosmos-kit-react.d.ts"
      },
      "development": {
        "module": "./dist/cosmos-kit-react.esm.js",
        "import": "./dist/cosmos-kit-react.esm.js",
        "default": "./dist/cosmos-kit-react.cjs.js"
      },
      "module": "./dist/cosmos-kit-react.esm.js",
      "import": "./dist/cosmos-kit-react.esm.js",
      "default": "./dist/cosmos-kit-react.cjs.js"
    },
    "./styles.css": "./dist/styles.css",
    "./package.json": "./package.json"
  },
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "types",
    "dist"
  ],
  "scripts": {
    "build": "parcel build --config .parcelrc",
    "watch": "parcel watch --config .parcelrc",
    "prepare": "npm run build",
    "dev": "parcel watch",
    "lint": "eslint --ext .tsx,.ts .",
    "format": "eslint --ext .tsx,.ts --fix .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "clear": "rm -rf ./dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cosmology-tech/cosmos-kit"
  },
  "keywords": [
    "cosmos",
    "cosmos-kit",
    "cosmostation",
    "leap",
    "keplr",
    "wallet"
  ],
  "bugs": {
    "url": "https://github.com/cosmology-tech/cosmos-kit/issues"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "main/",
      "module/",
      "types/"
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "^7.22.5",
    "@cosmjs/cosmwasm-stargate": "0.29.0",
    "@cosmjs/stargate": "0.29.0",
    "@parcel/config-default": "2.8.3",
    "@parcel/core": "2.8.3",
    "@parcel/transformer-js": "2.8.3",
    "@parcel/transformer-react-refresh-wrap": "2.8.3",
    "@parcel/transformer-typescript-tsc": "2.8.3",
    "@parcel/transformer-typescript-types": "2.8.3",
    "@types/jest": "29.0.0",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "cross-env": "7.0.2",
    "eslint": "8.23.1",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "jest": "29.0.2",
    "jest-in-case": "1.0.2",
    "parcel": "2.8.3",
    "prettier": "2.1.2",
    "regenerator-runtime": "0.13.7",
    "ts-jest": "29.0.1",
    "typescript": "4.8.3",
    "webpack": "5.74.0"
  },
  "dependencies": {
    "@chain-registry/types": "0.13.1",
    "@cosmology-ui/react": "1.1.0-beta.2",
    "@cosmos-kit/core": "^1.7.1-beta.2",
    "@cosmos-kit/react-lite": "^1.5.2-beta.2",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "framer-motion": "^9.0.7",
    "react": "18.2.0",
    "react-icons": "4.4.0"
  },
  "resolutions": {
    "@types/react": "^18.2"
  },
  "peerDependencies": {
    "@cosmjs/cosmwasm-stargate": ">= 0.28",
    "@cosmjs/stargate": ">= 0.28",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "gitHead": "11871f1b2de00eaea547e5aac24207ebea3276fe"
}
