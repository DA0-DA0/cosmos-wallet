    "@cosmos-kit/frontier": "^2.0.2",
    "@cosmos-kit/keplr": "^2.0.2",
    "@cosmos-kit/leap": "^2.0.2",
    "@cosmos-kit/ledger": "^2.0.2",
    "@cosmos-kit/fin": "^2.0.2",
    "@cosmos-kit/omni": "^2.0.2",
    "@cosmos-kit/shell": "^2.0.2",
    "@cosmos-kit/station": "^2.0.2",
    "@cosmos-kit/trust": "^2.0.2",
    "@cosmos-kit/vectis": "^2.0.2",
    "@cosmos-kit/xdefi": "^2.0.2",
